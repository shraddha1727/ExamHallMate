{
  "name": "spi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:web\"",
    "dev:api": "nodemon --watch './**/*.ts' --exec \"node --loader ts-node/esm -r dotenv/config server.ts\"",
    "dev:web": "vite --host 0.0.0.0 --port 3000",
    "dev:web:wait": "wait-on http://127.0.0.1:3001 && npm run dev:web",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node --loader ts-node/esm server.ts",
    "seed": "node --loader ts-node/esm -r dotenv/config scripts/seed.ts",
    "fix:teachers": "node --loader ts-node/esm -r dotenv/config scripts/fix_teachers.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lucide-react": "^0.555.0",
    "mongodb": "^7.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^7.2.0"
  }
}
